<TableData name="广元朝天区银行存款" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[REPORTJDBC]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT sum(accrual_org) 期末,
ack.name name,
left(ack.code,10) 代码,
concat(left(ack.code,10),"000") 名称,
sum(accrual_org)+sum(fb.open) 金额
FROM rpt_ps_voucher rpa left join
(select accbook,accsubject,localcreditamount,localdebitamount,
localdebitamount open from fi_balance 
where 1=1 and flag in ("00","01")) fb 
on fb.accbook = rpa.accbook and fb.accsubject = rpa.accsubject
left join bd_accbook ack on rpa.accbook = ack.id
where 1=1 
and rpa.accsubject_code like '102%' 
 and ack.code like "v513434%"
 group by 代码 
 order by ack.code]]></Query>
</TableData>
