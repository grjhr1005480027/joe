</Connection>
<Query>
<![CDATA[SELECT sum(accrual_org) 期末,
ack.name name,
sum(accrual_org)+sum(fb.open) 金额
FROM rpt_ps_voucher rpa left join
(select accbook,accsubject,localcreditamount,localdebitamount,localcreditamount- localdebitamount open from fi_balance where 1=1 and flag in ("00","01")) fb 
on fb.accbook = rpa.accbook and fb.accsubject = rpa.accsubject
left join bd_accbook ack on rpa.accbook = ack.id
where 1=1 
and rpa.accsubject_code like '102%' 
 and ack.code like "v513434%"
     ${if(len(name)=0 ,"and 1=2","and ack.code like '"+left(name,10)+"%'")}
 group by rpa.accbook 
]]></Query>
